hostname AZURE1
!
vtp domain cml
vtm mode transparent
!
vrf definition Mgmt-intf
!
 address-family ipv4
 exit-address-family
!
interface GigabitEthernet1
 no switchport
 vrf forwarding Mgmt-intf
 ip address 53.220.250.18 255.255.255.0
 no shut
!
username cisco privilege 15 secret cisco
ip ssh version 2
ip domain-name cml.local
!
crypto key generate rsa modulus 2048
!

ip route vrf Mgmt-intf 0.0.0.0 0.0.0.0 53.220.250.1
!
line vty 0 4
login local

